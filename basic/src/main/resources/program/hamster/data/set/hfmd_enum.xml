<dataset>
  <module>sec</module>
  <code>hfmd_enum</code>
  <entity>hfmd_enum</entity>
  <name>字典项【默认】</name>
  <fields>
    <field code="hfmd_enum_id" name="字典项ID" edit-type="input" is-key="true" not-null="true">
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>text</update-edit-type>
    </field>
    <field code="hfmd_enum_value" name="字典项值" edit-type="input"/>
    <field code="hfmd_enum_text" name="字典项文本" edit-type="input"/>
    <field code="hfmd_enum_desc" name="字典项描述" edit-type="input"/>
    <field code="is_default" name="是否默认" edit-type="select">
      <enum-class code="DEFAULT.BOOLEAN"/>
    </field>
    <field code="pri" name="优先级" edit-type="input">
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>hidden</update-edit-type>
    </field>
    <field code="ext1" name="扩展字段1" edit-type="input"/>
    <field code="ext2" name="扩展字段2" edit-type="input"/>
    <field code="hfmd_enum_class_id" name="字典ID" edit-type="select">
      <rel entity-code="hfmd_enum_class/hfmd_enum_class_id/hfmd_enum_class_name"/>
    </field>
    <field code="hfmd_enum_class_code" name="" edit-type="input"/>
    <field code="op_id" name="创建人" edit-type="input">
      <rel entity-code="hfsec_user/hfsec_user_id/hfsec_user_name"/>
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>text</update-edit-type>
    </field>
    <field code="create_time" name="创建时间" edit-type="datetime">
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>text</update-edit-type>
    </field>
    <field code="modify_op_id" name="修改人" edit-type="input">
      <rel entity-code="hfsec_user/hfsec_user_id/hfsec_user_name"/>
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>hidden</update-edit-type>
    </field>
    <field code="modify_time" name="修改时间" edit-type="datetime">
      <create-edit-type>hidden</create-edit-type>
      <update-edit-type>hidden</update-edit-type>
    </field>
    <field code="del_flag" name="删除标识" edit-type="input">
      <create-edit-type>hidden</create-edit-type>
      <enum-class code="DEFAULT.BOOLEAN"/>
    </field>
  </fields>
</dataset>
